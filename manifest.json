{
    "manifest_version": 3,
    "name": "健保雲端資料整理",
    "version": "1.0.18",
    "description": "ACT5代碼",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_icon": {
            "19": "icon19.png",
            "38": "icon38.png"
        },
        "default_title": "健保雲端藥歷分析器"
    },
    "options_page": "settings.html",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
        {
            "matches": [
                "https://medcloud2.nhi.gov.tw/*",
                "https://medvpn.nhi.gov.tw/*"
            ],
            "js": [
                "medicineProcessor.js",
                "labProcessor.js",
                "labAbbreviations.js",
                "atc5ColorManager.js",
                "imageProcessor.js",
                "autoPaging.js",
                "content.js"
            ],
            "css": ["style.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["icon16.png", "icon48.png", "icon128.png"],
            "matches": ["https://*.nhi.gov.tw/*"]
        }
    ]
  }